  <select id="selectUserAll" resultMap="BaseResultMap" parameterType="com.cn.exclusiveGirlfriend.dataBean.UserDataBean">
    select
    <include refid="Base_Column_List" />
    from user
    <where>
      <if test="user.name!=null and user.name!=''">
        name like '%'#{user.name,jdbcType=VARCHAR}'%',
      </if>
      <if test="user.sex!=null and user.sex!=''">
        and sex = #{user.sex,jdbcType=VARCHAR},
      </if>
      <if test="user.identity !=null and user.identity !=''">
        and identity = #{user.identity,jdbcType=VARCHAR},
      </if>
      <if test="user.phone!=null and user.phone!=''">
        and phone = #{user.phone,jdbcType=VARCHAR},
      </if>
      <if test="user.weChat!=null and user.weChat!=''">
        and weChat = #{user.wechat,jdbcType=VARCHAR},
      </if>
      <if test="user.qq!=null and user.qq!=''">
        and qq = #{user.qq,jdbcType=VARCHAR},
      </if>
      <if test="createtime!=null">
        and createtime &gt;=#{createtime}
      </if>
      <if test="endtime!=null">
